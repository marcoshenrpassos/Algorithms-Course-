algoritmo "fibonacci"

var
  num1, num2, cont, soma: inteiro
inicio
      num1 <- 0
      Escreva (num1)
      num2 <- 1
      Escreva (num2)
      Para cont <- 1 ate 13 faca
           soma <- num1 + num2
           num1 <- num2
           Escreva (soma)
           num2 <- soma
      FimPara
fimalgoritmo
