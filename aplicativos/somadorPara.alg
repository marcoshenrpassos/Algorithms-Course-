algoritmo "somadorPara"
// Função :
// Autor :
// Data : 28/01/2022
// Seção de Declarações 
var
    N, C, S: inteiro
inicio
// Seção de Comandos 
   S <- 0
   Para C <- 1 ate 5 faca
        Escreval ("Digite um valor: ")
        Leia (N)
        S <- S + N
   FimPara
   Escreval ("A soma dos valores e: ", S)
fimalgoritmo
