algoritmo "SomaFuncao"

var
   V1, V2, R: inteiro

funcao Soma(A, B: inteiro) : inteiro
inicio
      retorne A + B

fimFuncao

inicio
      Escreva("Digite o primeiro valor: ")
      leia (V1)
      Escreva("Digite o segundo valor: ")
      leia (V2)
      R <- Soma(V1,V2)
      Escreva("a sma entre o numero ", V1, " e o numero ", V2,  " e ", R)
fimalgoritmo
