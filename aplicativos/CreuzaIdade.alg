algoritmo "CreuzaIdade"
// Função :
// Autor :
// Data : 24/01/2022
// Seção de Declarações 
var
   anoAtual,anoNasc,idade: inteiro
inicio
// Seção de Comandos 
   Escreva("Em que ano estamos? ")
   leia(anoAtual)
   Escreva("Em que ano eu nasci? ")
   leia(anoNasc)
   idade <- anoAtual - anoNasc
   Escreva("Minha idade é", idade)
fimalgoritmo
